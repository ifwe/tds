#!/usr/bin/env python

import re
import sys
import yaml

def main(rpm_format, filename):
    data = yaml.load(open(filename))

    def sub(x):
        return str(data.get(x.group(1), ''))

    print re.sub(r'%\{(.*?)}', sub, rpm_format)
    return 0


if __name__ == '__main__':
    sys.exit(main(*sys.argv[1:]))
